contract RopeDepthsUpdateWrapper {
    data {
        StartedIds string
        StartedValues string
        FinalIds string
        FinalValues string
    }

    conditions {}

    action {
        RopeDepthsUpdate(
            "StartedIds,StartedValues,FinalIds,FinalValues",
            JSONDecode($StartedIds),
            JSONDecode($StartedValues),
            JSONDecode($FinalIds),
            JSONDecode($FinalValues)
        )
    }
}