contract VotingSysparamInit{
    data{
        Name string
        Value string
    }

    conditions{
        if GetContractByName($Name) == 0 {
            warning "System parameter not found"
        }
    }
	
	action{
        var params map
        params["Value"] = $Value
        CallContract($Name, params)
        // InvestorRefresh()	
	}
}