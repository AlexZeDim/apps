Form(input-group){
    Input(Name: Search, Value: #Search#, Placeholder: Enter text to search)
    Div(input-group-btn){
        If(GetVar(Search)!=""){
            Button(Class: btn btn-default fa fa-close, Page: #this_page#)
        }
        Button(Class: btn btn-default fa, Page: #this_page#, PageParams: "Search=Val(Search)", Body: Em(Class: fa fa-search) Search)
    }
}