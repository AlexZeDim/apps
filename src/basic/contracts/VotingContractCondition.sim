contract VotingContractCondition {
    data {
    }

    conditions {
        $runningVoting = DBFind("@1votings").Where({status:{"$in":[1, 2, 3]}, "voting->name":{"$like":"Template ID"}, "voting->type": 2, "voting->description":{"$like":"Edit contract"}}).Row()
        if $runningVoting{
            error "Can not editing this contract while voting not finished"
        }
    }

    action {
    }
}