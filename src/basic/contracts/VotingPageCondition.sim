contract VotingPageCondition {
    data {
    }

    conditions {
        $runningVoting = DBFind("@1votings").Where({status:{"$in":[1, 2, 3]}, "voting->name":{"$like":"Template ID"}, "voting->type": 2, "voting->description":{"$like":"Edit page"}}).Row()
        if $runningVoting{
            error "Can not editing this page while voting not finished"
        }
    }

    action {
    }
}