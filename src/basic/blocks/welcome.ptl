Div(content-wrapper){
    Div(panel panel-primary){
        Div(list-group-item text-center){
            P(Class: h3 m0 text-bold, Body: $@1welcome_title$)
        }
        Div(list-group-item){
            Span(Class: h4, Body: $@1welcome_text$)
            Span(Class: h4 text-primary, Body: " https://genesiskernel.readthedocs.io")
        }
        Div(panel-footer text-right clearfix){
            Button(Body: $@1install$, Class: btn btn-primary, Page: @1import_upload)
        }
    }
}